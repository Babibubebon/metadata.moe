<ISDNShape_ISDNコード> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "ISDNコード"@ja ;
    sh:nodeKind sh:Literal ;
    sh:path schema1:value .

<ISDNShape_グループ名称> a sh:PropertyShape ;
