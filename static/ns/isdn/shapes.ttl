    sh:name "容量"@ja ;
    sh:node <QuantitativeValueShape> ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:path isdn:carrierExtent .

<DoujinProductShape_形態> a sh:PropertyShape ;
    sh:datatype xsd:string ;
